{
  "name": "poe-app-frontend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "source": [
    "src/js/index.js",
    "src/css/index.scss"
  ],
  "targets": {
    "default": {
      "distDir": "./_site/assets"
    }
  },
  "scripts": {
    "clean": "rimraf _site",
    "dev": "cross-env NODE_ENV=development npm-run-all clean --parallel develop:*",
    "develop:eleventy": "eleventy --quiet --serve --watch",
    "develop:parcel": "parcel watch --no-cache",
    "build": "cross-env NODE_ENV=production npm-run-all clean prod:parcel prod:eleventy",
    "prod:eleventy": "eleventy --quiet",
    "prod:parcel": "parcel build --no-source-maps --log-level verbose",
    "serve": "eleventy --quiet --serve"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "Firefox ESR",
    "not IE < 12",
    "not ie_mob < 12",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "@parcel/transformer-sass": "2.7",
    "@ronilaukkarinen/stylelint-a11y": "^1.2.4",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "eleventy-plugin-purgecss": "^0.3.0",
    "eslint": "^8.28.0",
    "events": "^3.3.0",
    "glob": "^8.0.3",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "postcss-preset-env": "^7.8.3",
    "postcss-sort-media-queries": "^4.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "svg-sprite": "^2.0.2",
    "svgo": "^3.0.2",
    "vinyl": "^3.0.0"
  },
  "dependencies": {
    "modern-normalize": "^1.1.0",
    "rfs": "^9.0.6",
    "simple-scrollspy": "^2.3.3",
    "vanilla-lazyload": "^17.8.3",
    "yt-player": "^3.6.1"
  }
}
