{
  "name": "poe-app-frontend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf _site",
    "dev": "cross-env NODE_ENV=development npm-run-all clean --parallel develop:*",
    "develop:eleventy": "eleventy --quiet --serve --watch",
    "develop:rollup": "rollup -c -w",
    "build": "cross-env NODE_ENV=production npm-run-all clean --parallel prod:*",
    "prod:eleventy": "eleventy --quiet",
    "prod:postcss": "postcss src/assets/css/index.css -o _site/css/index.css --config ./postcss.config.js"
  },
  "browserslist": [
    "last 2 versions",
    "> 1% in RU",
    "not IE < 12",
    "not ie_mob < 12",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "cssnano": "^5.0.6",
    "glob": "^7.1.7",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "rimraf": "^3.0.2",
    "svg-sprite": "^1.5.0",
    "svgo": "^2.3.1",
    "tailwindcss": "^2.2.4",
    "vinyl": "^2.2.1",
    "cross-env": "^7.0.3",
    "rollup": "^2.53.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "gumshoejs": "^5.1.2",
    "vanilla-lazyload": "^17.4.0"
  }
}
