{
  "name": "poe-app-frontend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf _site",
    "dev": "cross-env NODE_ENV=development npm-run-all clean --parallel develop:*",
    "develop:eleventy": "eleventy --quiet --serve --watch",
    "develop:rollup": "rollup -c -w",
    "build": "cross-env NODE_ENV=production npm-run-all clean prod:rollup prod:eleventy",
    "prod:eleventy": "eleventy --quiet",
    "prod:rollup": "rollup -c",
    "serve": "eleventy --quiet --serve"
  },
  "browserslist": [
    "last 2 versions",
    "not IE < 12",
    "not ie_mob < 12",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "eleventy-plugin-purgecss": "^0.1.1",
    "eslint": "^7.30.0",
    "glob": "^7.1.7",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.35.2",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "svg-sprite": "^1.5.0",
    "svgo": "^2.3.1",
    "vinyl": "^2.2.1"
  },
  "dependencies": {
    "modern-normalize": "^1.1.0",
    "rfs": "^9.0.4",
    "simple-scrollspy": "^2.1.0",
    "vanilla-lazyload": "^17.4.0",
    "yt-player": "^3.5.0"
  }
}
