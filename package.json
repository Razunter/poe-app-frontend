{
  "name": "poe-app-frontend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "source": [
    "src/js/index.js",
    "src/css/index.scss"
  ],
  "targets": {
    "default": {
      "distDir": "./_site/assets"
    }
  },
  "scripts": {
    "clean": "rimraf _site",
    "dev": "cross-env NODE_ENV=development npm-run-all clean --parallel develop:*",
    "develop:eleventy": "eleventy --quiet --serve --watch",
    "develop:parcel": "parcel watch",
    "build": "cross-env NODE_ENV=production npm-run-all clean prod:parcel prod:eleventy",
    "prod:eleventy": "eleventy --quiet",
    "prod:parcel": "parcel build --no-source-maps",
    "serve": "eleventy --quiet --serve"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "not IE < 12",
    "not ie_mob < 12",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-canary.39",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "@parcel/transformer-sass": "^2.0.0",
    "autoprefixer": "^10.3.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "eleventy-plugin-purgecss": "^0.1.1",
    "eslint": "^8.0.1",
    "glob": "^7.2.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.43.2",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "svg-sprite": "^1.5.3",
    "svgo": "^2.7.0",
    "vinyl": "^2.2.1"
  },
  "dependencies": {
    "modern-normalize": "^1.1.0",
    "rfs": "^9.0.6",
    "simple-scrollspy": "^2.2.0",
    "vanilla-lazyload": "^17.5.0",
    "yt-player": "^3.5.0"
  }
}
